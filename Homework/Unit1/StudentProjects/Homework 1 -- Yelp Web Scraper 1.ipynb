   "source": [
    "#new while loop with final price range code -\n",
    "#looking at the output - doens't quite work \n",
    "\n",
    "i = 0\n",
    "rest_price_range = []\n",
    "while i < 210:\n",
    "    \n",
    "    #define website to scrape, and scraped text \n",
    "    url = f'https://www.yelp.com/search?find_desc=Restaurants&find_loc=London%2C%20United%20Kingdom&ns=31&start={i}'\n",
    "    req = requests.get(url)\n",
    "    yelp_scraper = BeautifulSoup(req.text)\n",
    "   \n",
    "    price_range = yelp_scraper.find_all('span', {'class': 'lemon--span__373c0__3997G', \n",
    "                                        'class': 'text__373c0__2Kxyz',\n",
    "                                        'class': 'reviewCount__373c0__2r4xT',\n",
    "                                        'class': 'text-color--black-extra-light__373c0__2OyzO'})\n",
    "\n",
    "    price_range = [str(range_) for range_ in price_range]\n",
    "    price_range = [range_.replace('</span>', '') for range_ in price_range]\n",
    "    price_range = [range_.split('>')[1] for range_ in price_range]\n",
    "\n",
    "    #new code to remove the HTML code\n",
    "    new_price_range = []\n",
    "    for range in price_range:\n",
    "        if not ('<a' in range):\n",
    "            new_price_range.append(range)\n",
    "    \n",
    "    #new code to add the null value\n",
    "    \n",
    "    for i, range, in enumerate(new_price_range,1):\n",
    "        if i % 2 == 1 and not range.isdigit():\n",
    "            new_price_range.insert(i, None)\n",
    "    \n",
    "    \n",
    " \n",
    "   #print price range - and skip every other item (which is the review number)\n",
    "    print(new_price_range[1::2])\n",
    "   #introduced this after the fact to error check why not 30 items\n",
    "    print('page range', i,'-- count of items:', len(new_price_range[1::2]))\n",
    "    \n",
    "    #add to new and final list\n",
    "    rest_price_range.extend(new_price_range[1::2])\n",
    "    \n",
    "    i = i + 30\n"
   ]
